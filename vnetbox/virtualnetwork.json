{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "envGuid":{
            "type": "string",
            "defaultvalue": ""
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
                {
            "apiVersion": "2020-07-01",
            "name": "[concat('lb-int-',parameters('envGuid'))]",
            "type": "Microsoft.Network/loadBalancers",
            "location": "[resourceGroup().location]",
            "sku": {
                "name": "Standard"
            },
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', concat('vnet-env-',parameters('envGuid')))]"
            ],
            "tags": {
                "envGuid": "[parameters('envGuid')]"
            },
            "properties": {
                "frontendIPConfigurations": [
                    {
                        "name": "lb-FrontEnd",
                        "properties": {
                            "privateIPAllocationMethod": "Static",
                            "subnet": {
                                "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', concat('vnet-env-',parameters('envGuid')), 'Subnet-255-lb-fe')]"
                            },
                            "privateIPAddress": "10.0.255.5",
                            "privateIPAddressVersion": "IPv4"
                        }
                    }
                ]
            }
        },

        {
            "name": "[concat('vnet-env-',parameters('envGuid'))]",
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2019-11-01",
            "location": "[resourceGroup().location]",
            "tags": {
                "displayName": "vnet-env",
                "envGuid": "[parameters('envGuid')]"
            },
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.0.0.0/16"
                    ]
                },
                "subnets": [
                    {
                        "name": "Subnet-0",
                        "properties": {
                            "addressPrefix": "10.0.0.0/24"
                        }
                    },
                    {
                        "name": "Subnet-1",
                        "properties": {
                            "addressPrefix": "10.0.1.0/24"
                        }
                    },
                    {
                        "name": "Subnet-255-lb-fe",
                        "properties": {
                            "addressPrefix": "10.0.255.0/24",
                            "privateLinkServiceNetworkPolicies": "Disabled"
                        }
                    }

                ]
            }
        },
        {
            "apiVersion": "2019-04-01",
            "type": "Microsoft.Network/privateLinkServices",
            "name": "[concat('privlink-',parameters('envGuid'))]",
            "location": "[resourceGroup().location]",
            "tags": {
                "env": "guid"
            },
            "properties": {
                "visibility": {
                    "subscriptions": []
                },
                "autoApproval": {
                    "subscriptions": []
                },
                "enableProxyProtocol": false,
                "loadBalancerFrontendIpConfigurations": [
                    {
                        "id": "[resourceId('Microsoft.Network/loadBalancers/frontendIpConfigurations',concat('lb-int-',parameters('envGuid')),'lb-FrontEnd' )]"
                    }
                ],
                "ipConfigurations": [
                    {
                        "name": "privlink-IP",
                        "properties": {
                            "privateIPAllocationMethod": "Static",
                            "privateIPAddress": "10.0.255.10",
                            "privateIPAddressVersion": "IPv4",
                            "subnet": {
                                "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', concat('vnet-env-',parameters('envGuid')), 'Subnet-255-lb-fe')]"
                            },
                            "primary": true
                        }
                    }
                ],
                "privateEndpointConnections": []
            },
            "dependsOn": [
                "[resourceId('Microsoft.Network/loadBalancers', concat('lb-int-',parameters('envGuid')))]"
            ]
        }
    ],
    "outputs": {}
}