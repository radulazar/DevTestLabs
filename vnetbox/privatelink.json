{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "envGuid": {
            "type": "string",
            "metadata": {
                "description": "description"
            },
            "defaultValue":""
        },
        "vmIndex": {
            "type": "string",
            "metadata": {
                "description": "description"
            }
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
                {
            "location": "[resourceGroup().location]",
            "name": "[concat('priv-endpoint-',parameters('vmIndex'),'-',parameters('envGuid'))]",
            "type": "Microsoft.Network/privateEndpoints",
            "apiVersion": "2020-03-01",
            "properties": {
                "subnet": {
                    "id": "/subscriptions/d8058b04-7da0-4499-b527-029c6d0158f7/resourceGroups/OnPremConnect-rg/providers/Microsoft.Network/virtualNetworks/OnPremConnect-vNet/subnets/192.168.1.0_24"
                },
                "privateLinkServiceConnections": [
                    {
                        "name": "[resourceId('Microsoft.Network/privateEndpoints',concat('priv-endpoint-',parameters('envGuid')))]",
                        "properties": {
                            "privateLinkServiceId": "[resourceId('Microsoft.Network/privateLinkServices',concat('privlink-',parameters('vmIndex'),'-',parameters('envGuid')))]"

                        }
                    }
                ]
            },
            "tags": {
                "env": "guid"
            }
        }
    ],
    "outputs": {
        "privEndpoint-NIC": {
            "type": "object",
            "value": "[reference(resourceId('Microsoft.Network/privateEndpoints',concat('priv-endpoint-',parameters('vmIndex'),'-',parameters('envGuid')))).networkInterfaces[0]]"
        }
    }
}